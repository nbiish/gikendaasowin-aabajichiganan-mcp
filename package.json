{
  "name": "@nbiish/cognitive-tools-mcp",
  "version": "0.3.3",
  "description": "A Model Context Protocol server providing advanced cognitive reasoning tools for AI agents",
  "private": false,
  "type": "module",
  "bin": {
    "cognitive-tools-mcp": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js",
    "start": "node build/index.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai",
    "reasoning",
    "cognitive-tools",
    "claude",
    "agent"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nbiish/cognitive-tools-server.git"
  },
  "author": "ᓂᐲᔥ ᐙᐸᓂᒥᑮ-ᑭᓇᐙᐸᑭᓯ (Nbiish Waabanimikii-Kinawaabakizi)",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.8.0",
    "ajv": "^8.17.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.17.28",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
